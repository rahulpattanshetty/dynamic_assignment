<%= form_for User.new, url: admin_users_path do |form| %>
  <div class="form-group">
    <%=form.label :email%>
    <%= form.text_field :email, class:"form-control"%>
  </div>
  <div class="form-group">
    <%=form.label :password%>
    <%= form.text_field :password, class:"form-control"%>
  </div>
  <%=form.submit nil%>
<%end%>